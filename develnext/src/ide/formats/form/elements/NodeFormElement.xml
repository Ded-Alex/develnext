<element>
    <init>
        <property code="visible" virtual="1" value="1" />
        <property code="enabled" virtual="1" value="1" />
    </init>

    <properties group="general" title="Главное" />

    <properties group="extra" title="Дополнительно">
        <property code="id" name="ID" editor="id" tooltip="Уникальный индетификатор элемента в рамках формы" />
        <property code="classesString" name="CSS класс(ы)" editor="text" tooltip="Класс или несколько классов через пробел" />

        <property code="cursor" name="Курсор" editor="enum" tooltip="Тип курсора при наведении на объект" virtual="1" defaultValue="DEFAULT">
            <variants>
                <variant value="DEFAULT">[DEFAULT] По-умолчанию</variant>
                <variant value="HAND">[HAND] Указательный палец</variant>
                <variant value="TEXT">[TEXT] Редактирование</variant>
                <variant value="WAIT">[WAIT] Ожидание</variant>
                <variant value="MOVE">[MOVE] Перемещение</variant>
                <variant value="NONE">[NONE] Отсутствует</variant>
            </variants>
        </property>

        <property code="visible" name="Видимость" editor="boolean" virtual="1" tooltip="Видимость элемента на форме в момент старта" />
        <property code="enabled" name="Доступность" editor="boolean" virtual="1" tooltip="Доступность элемента на форме в момент старта" />

        <property code="x" name="Позиция X" editor="integer" />
        <property code="y" name="Позиция Y" editor="integer" />

        <property code="width" name="Ширина" editor="integer" />
        <property code="height" name="Высота" editor="integer" />
    </properties>

    <eventTypes>
        <eventType code="click" name="Клик мышки" kind="MouseEvent" icon="icons/mouseEx16.png" />

        <eventType code="mouseDown" name="Нажатие мышки" kind="MouseEvent" icon="icons/mouse16.png" />
        <eventType code="mouseMove" name="Движение мышки" kind="MouseEvent" icon="icons/mouse16.png" />
        <eventType code="mouseUp" name="Отжатие мышки" kind="MouseEvent" icon="icons/mouse16.png" />

        <eventType code="mouseEnter" name="Курсор на объекте" kind="MouseEvent" icon="icons/cursor16.png" />
        <eventType code="mouseExit" name="Курсор вне объекта" kind="MouseEvent" icon="icons/cursor16.png" />

        <eventType code="keyDown" name="Нажатие клавиши" kind="KeyEvent" icon="icons/key16.png" />
        <eventType code="keyPress" name="Клавиатура" kind="KeyEvent" icon="icons/keyboard16.png" />
        <eventType code="keyUp" name="Отжатие клавишы" kind="KeyEvent" icon="icons/key16.png" />
    </eventTypes>
</element>