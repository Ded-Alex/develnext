<behaviour extends="BehaviourSpec">
    <properties>
        <property name="Тип объекта" code="bodyType" editor="enum" tooltip="Тип объекта в физическом мире">
            <variants>
                <variant value="STATIC">Статичный</variant>
                <variant value="DYNAMIC">Динамичный</variant>
            </variants>
        </property>

        <property name="Тип фигуры" code="fixtureType" editor="enum" tooltip="От фигуры объекта зависит просчет столкновений, по-умолчанию означает использовать фигуру спрайта">
            <variants>
                <variant value="INHERITED">[По-умолчанию]</variant>
                <variant value="RECTANGLE">Прямоугольник</variant>
                <variant value="ELLIPSE">Эллипс, овал</variant>
            </variants>
        </property>

        <property code="velocity" name="Скорость (X, Y)" editor="doubleArray" tooltip="Начальная скорость по X и Y" />
        <!-- <property code="solid" name="Твердый" editor="boolean" />  -->
    </properties>
</behaviour>